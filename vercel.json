{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "DATABASE_PATH": "/tmp/bot.db",
    "ADMIN_WEB_USER": "admin",
    "ADMIN_WEB_PASS": "change-me"
  }
}
